---
import Layout from "../../layouts/Layout.astro";
import { getEntry } from "astro:content";
const { slug } = Astro.params;

if(slug === undefined){
    throw new Error('Slug required')
}

const entry = await getEntry('blog', slug);

if(entry === undefined){
    return Astro.redirect('/404')
}

